{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Svelte",
  "compilerOptions": {
    "moduleResolution": "nodenext",
    "target": "es2017",
    "importsNotUsedAsValues": "error",
    "preserveValueImports": true,
    "isolatedModules": true,
    "sourceMap": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
