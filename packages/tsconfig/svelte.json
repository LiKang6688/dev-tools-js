{
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "target": "ES2017",
    "moduleResolution": "node",
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    "sourceMap": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Svelte"
}
